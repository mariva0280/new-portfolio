{
  "name": "new-portfolio-frontend",
  "scope": "mariva0280",
  "alias": ["new-portfolio-frontend"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

  